
@{
    ViewBag.Title = "AllMessages";
    Layout = "~/Views/Shared/_helpd.cshtml";
}

 
<div style="margin-bottom:10%;"></div>
<div class="form-horizontal" style="width:90%">
    <ol class="breadcrumb">
        <li><i class="fa fa-envelope-o"></i><a href="#">Notifications</a></li>
    </ol>
</div>
<div class="box box-primary">
</div>
<div class="box box-primary">
    <div class="box-body">
        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#home"><i class="fa fa-envelope"></i>&nbsp;&nbsp; Notifications</a></li>

            <li><a data-toggle="tab" href="#menu2"><i class="fa fa-mail-reply-all"></i>&nbsp;&nbsp;Opened Notifications</a></li>


        </ul>
        <div class="tab-content">
            <div id="home" class="tab-pane fade in active">
                @{
                    Html.RenderAction("Inbox");
                }
            </div>
            <div id="menu2" class="tab-pane fade">
                @{
                    Html.RenderAction("OpenedMail");
                }
            </div>

        </div>
    </div>
</div>


@model List<AssetManagement.Business.HelpDeskSystem.TicketReportPerParticipant>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_helpd.cshtml";
    AssetManagement.Business.HelpDeskSystem.TicketReportPerParticipant AllTickets = Model[0];
    AssetManagement.Business.HelpDeskSystem.TicketReportPerParticipant AdminTickets = Model[1];
}
<div style="margin-bottom:10%;"></div>
<div class="form-horizontal" style="width:90%">
    <ol class="breadcrumb">
        <li><i class="fa fa-gears"></i><a href="#">Tickets Control</a></li>
    </ol>
</div>
<div class="box box-primary">
</div>
<div class="box box-primary">
    <div class="box-body">
        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#all">All Tickets <span class="badge">@AllTickets.All.Count</span></a></li>
            <li><a data-toggle="tab" href="#myTickets">My Ticket <span class="badge">@AdminTickets.All.Count</span></a></li>
            <li><a data-toggle="tab" href="#mySolutions">My Solutions <span class="badge">@ViewBag.MySolutions</span></a></li>
            <li><a data-toggle="tab" href="#KnwolageBase">Knowledge Base <span class="badge">@ViewBag.KnowledgeBase</span></a></li>
            <li><a data-toggle="tab" href="#OverdueTickets">Overdue Tickets <span class="badge">@ViewBag.OverDueTickets</span></a></li>
        </ul>
        <div style="margin-bottom:2%;"></div>
        <div class="tab-content">
            <div id="all" class="tab-pane fade in active">
                @{
                    Html.RenderPartial("_TicketsPerParticipant", AllTickets);
                }
            </div>
            <div id="myTickets" class="tab-pane fade">
                @{
                    Html.RenderPartial("_AdminTickets", AdminTickets);
                }
            </div>
            <div id="mySolutions" class="tab-pane fade">
                @{
                    Html.RenderAction("MySolutions");
                }
            </div>
            <div id="KnwolageBase" class="tab-pane fade">
                @{
                    Html.RenderAction("Solutions");
                }
            </div>
            <div id="OverdueTickets" class="tab-pane fade">
                @{
                    Html.RenderAction("OverDueTickets");
                }
            </div>
        </div>
    </div>
</div>

